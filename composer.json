{
    "name": "xakki/reflection-info",
    "version": "1.1",
    "description": "PHP - Reflection information from object",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Xakki",
            "email": "dev@xakki.ru"
        }
    ],
    "autoload": {
      "psr-4": {
        "Xakki\\ReflectionInfo\\": "src/"
      }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.86"
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "cs-check": "XDEBUG_MODE=off php-cs-fixer fix src --dry-run --diff",
        "cs-fix": "XDEBUG_MODE=off php-cs-fixer fix src --diff"
    }
}
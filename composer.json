{
    "name": "xakki/reflection-info",
    "version": "0.1",
    "description": "Reflection information",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "xakki",
            "email": "dev@xakki.ru"
        }
    ],
    "autoload": {
      "psr-4": {
        "Xakki\\ReflectionInfo\\": "src/"
      }
    },
    "require": {
        "php": "5.6.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13"
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "cs-check": "XDEBUG_MODE=off phpcs -ps",
        "cs-fix": "XDEBUG_MODE=off phpcbf -p"
    }
}